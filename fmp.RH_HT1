;HH NOE experiment (TopSpin 2.3)

;######################################################
;#                                                    #
;#  HN NOESY for solids                                #
;#                                                    #
;######################################################
;
;$COMMENT= HH NOESY Transfer Experiment
;$CLASS=Solids
;$DIM=2D
;$TYPE= 1H detect Hetero
;$SUBTYPE=Dipolar Transfer
;$OWNER=guest
;
#include <HCN_defs.incl>

;######################################################
;#                   Define Mixing                    #
;#    NOESY is Default, add Selective 90 w/ option    #
;######################################################

#include <fmp.HNOE.subr>

;cnst11 : adjust t=0 for acq, if digmod = baseopt
"acqt0=1u*cnst11"

define delay ONTIME
                            ;##########################
;$EXTERN                    ;# python insertion point #
                            ;##########################


Prepare, ze

"ONTIME=aq"                 ;#  DARR left out   #

;######################################################
;#               Protections: Pre-Check               #
;######################################################

#include <1H_ON_prot.incl>
        ;total RF depsosition restriction

;######################################################
;#           Start of Active Pulse Program            #
;######################################################

Start, d1 do:C do:N

#ifdef SInv
  5u
  (p24:spf24 pl24 ph0):H
  5u
  vd
#endif

#ifndef SInv
  (p4 pl2 ph0):H
  5u
  vd
#endif

  (p3 pl2 ph1):H
  
#ifdef echo
  3u
  
  d6
  (p4 pl2 ph2):H
  d6
  
  3u
#endif

;#######################################################
;#                     Acquisition                     #
;#######################################################

  0.5u pl3:N pl4:C     ; GARP power on 15N
  1u  cpd3:N cpd4:C

  go=Start ph31
  1m do:N do:C

  30m mc #0 to Start F1QF(ivd) 
 
HaltAcqu, 1m
exit

;#######################################################
;#             Phase Cycle (min=4, full=16)            #
;#######################################################

ph0=  2 1 0 3 
ph1=  0 2
ph2=  0 2 1 3
ph20= 0

#ifdef echo
ph31= 0 2 2 0
#endif

#ifndef echo
ph31= 0 2
#endif

;PARAMETERS:
;p1      : C 90  (pl1)
;p2      : C 180 (pl1)
;p3      : H 90  (pl2)
;p8      : C Presat (-DCpre)
;p6      : C soft 90  (pl26)
;p10     : HC CP contact pl10 (C) and sp0 (H)
;sp0     : H CP pwr
;pl1     : C HP pwr
;pl2     : H HP pwr
;pl3     : N dec pwr
;pl4     : C dec pwr
;pl10    : C CP pwr
;pl12    : H dec pwr
;pl13    : H alt dec pwr (Spinlock and CW)
;pl16    : H DARR pwr (DARR = MAS freq; PDSD =120)
;pl26    : C soft 90 pwr
;d1      : recycle delay
;d8      : PDSD/DARR mix time
;pcpd2   : pulse length in decoupling sequence
;cpdprg2 : cw, tppm (at pl12), or lgs, cwlg. cwlgs (LG-decoupling 
;spnam0  : use e.g. ramp.100 for variable amplitude CP
;spnam6  : C soft 90 purge pulse (e.g. eSNOB)
;cnst30  : expected td1 for use in constant Duty (-DCDC)
;cnst31  : MAS Rate
;FnMode  : TPPI, States or States-TPPI
;zgoptns : (-D) PRE, PURGE
