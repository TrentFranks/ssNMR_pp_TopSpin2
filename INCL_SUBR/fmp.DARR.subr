;#############################
;#  INCLUDE THESE PARAMETERS #
;#############################
;pl16   : 1H ampl (120 for PDSD)
;d8     : PDSD/DARR mixing time

subroutine PDSD_DARR()
{
  1u pl16:H 			
  1u cw:H 
  TauRm2
  d8				
  TauRm2
  2u do:H   
}

subroutine DARR_d9()
{
  1u pl16:H 			
  1u cw:H 
  TauRm2
  d9				
  TauRm2
  2u do:H   
}

subroutine DARRs6_(phase pha1)
{
  2u
  (p6:spf6 pl26 pha1):C  (p6 pl12):H 
  1u pl16:H 			
  TauRm4
  1u cw:H 
  d8				
  TauRm2
  2u do:H   
}

